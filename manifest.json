{
  "manifest_version": 2,
  "name": "Hella basic example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1",
  "browser_action": {
    "default_icon": "dildo-in-header.png"
  },
  "permissions": ["history"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ]
}
